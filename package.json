{
  "name": "@fabrigeas/react-form-group",
  "version": "3.0.9",
  "private": false,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/autosize": "^3.0.7",
    "@types/chai": "^4.2.11",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.45",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.0.0",
    "autosize": "^4.0.2",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "typescript": "^3.7.5",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:lib": "npm run build:clean && npm run build:babel && npm run build:types && node ./scripts/copyTS.js",
    "build:babel": "concurrently \"npm run build:babel:esm && npm run build:babel:umd\" \"npm run build:babel:cjs\"",
    "build:babel:cjs": "cross-env BABEL_ENV=production=cjs babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/js --presets=@babel/env",
    "build:babel:esm": "cross-env BABEL_ENV=production=esm babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/esm",
    "build:babel:umd": "cross-env BABEL_ENV=production=umd babel --source-maps --extensions \".js\" dist/esm --out-dir dist/umd --plugins=transform-es2015-modules-umd",
    "build:types": "tsc -p tsconfig.gen-dts.json",
    "build:clean": "rimraf build",
    "develop": "npm run build:types && npm run build:babel:esm --skip-initial-build --watch --verbose",
    "rollup": "npm run build:clean && npm run build:rollup",
    "build:rollup": "rollup -c",
    "build:start": "rollup -c -w"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "## A vue component that ressembles bootstraps formGroup + formControl",
  "types": "build/js/index.d.ts",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "prerelease"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-typescript-to-proptypes": "^1.3.2",
    "babel-runtime": "^6.26.0",
    "rollup": "^2.15.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.27.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabrigeas/react-formg-group.git"
  },
  "keywords": [
    "react",
    "form",
    "HTMLFormInputElements",
    "enzymes",
    "chai",
    "scss",
    "typescript"
  ],
  "author": "Fabrice Feugang Kemegni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabrigeas/react-formg-group/issues"
  },
  "homepage": "https://github.com/fabrigeas/react-formg-group#readme"
}
